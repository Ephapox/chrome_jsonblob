{
  "manifest_version": 2,
  "name": "jsonblob",
  "version": "0.0.1",
  "description": "Create JSON responses using https://jsonblob.com",
  "icons": {
  },
  "browser_action": {
    "default_icon": {
    }
  },
  "background": {
    "scripts": ["/build/event-page.bundle.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": ["/build/content-script.bundle.js"]
    }
  ],
  "permissions": [
    "storage",
    "*://jsonblob.com/*",
    "*://*/*"
  ]
}
